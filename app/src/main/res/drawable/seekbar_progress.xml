<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Background -->
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#1E1E1E"/> <!-- Background color -->
            <corners android:radius="8dp"/> <!-- Make it circular -->
        </shape>
    </item>
    <!-- Progress -->
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF"/> <!-- Progress color -->
            <corners android:radius="8dp"/> <!-- Make it circular -->
        </shape>
    </item>
</layer-list>
